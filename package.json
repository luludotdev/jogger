{
  "name": "@lolpants/jogger",
  "version": "0.7.1",
  "packageManager": "yarn@3.2.3",
  "description": "Elegant JSON logging system",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "check": "tsc --noEmit --skipLibCheck",
    "lint": "prettier --check . && eslint src --ext ts",
    "format": "prettier --write . && eslint src --ext ts --fix",
    "fmt": "yarn run format",
    "test": "yarn run check && yarn run lint",
    "prepack": "yarn run test && yarn run build"
  },
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lolPants/jogger.git"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "devDependencies": {
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16",
    "eslint": "^8.23.1",
    "eslint-config-neon": "^0.1.33",
    "prettier": "^2.7.1",
    "tsup": "^6.2.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "async-mutex": "^0.3.2",
    "globby": "^13.1.2",
    "json-colorizer": "^2.2.2",
    "mkdirp": "^1.0.4",
    "tslib": "^2.4.0"
  }
}
